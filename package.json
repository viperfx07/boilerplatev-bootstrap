  "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Entry files",
    "host": "127.0.0.1",
    "port": "3000",
    "baseUrl": "./",
    "directories": {
      "source": "src",
      "destination": "./",
      "otherWWW": [
        "../test-multisite/WWW_Shared/",
        "../test-multisite/WWW_Site_A/"
      ],
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "assets": "assets",
      "modules": "_modules",
      "layouts": "_layouts",
      "images": "_img",
      "icons": "_icons",
      "fonts": "_fonts",
      "styles": "_css",
      "scripts": "_js",
      "data": "_data"
    },
    "entries": {
      "js": "main.js",
      "css": "main**.{sass,scss}"
    }
  },
  "dependencies": {
    "jquery-match-height": "latest",
    "simplestatemanager": "latest"
